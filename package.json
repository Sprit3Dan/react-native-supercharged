{
  "name": "react-native-supercharged",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.3.1",
    "react-native": "0.55.2",
    "tsconfig-paths-webpack-plugin": "^3.0.3"
  },
  "devDependencies": {
    "@types/jest": "^22.0.1",
    "@types/react": "^16.0.5",
    "@types/react-native": "^0.51.0",
    "babel-jest": "22.4.3",
    "babel-loader": "^7.1.2",
    "babel-preset-react-native": "4.0.0",
    "haul": "1.0.0-beta.14",
    "jest": "22.4.3",
    "prettier": "^1.10.2",
    "react-test-renderer": "^16.3.1",
    "ts-jest": "^22.0.1",
    "ts-loader": "^3.5.0",
    "typescript": "^2.0.0"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
		"testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "useTabs": true,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "trailingComma": "es5"
  }
}
